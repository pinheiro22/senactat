!Comentários no Cisco IOS

!Configuraçao do Switch Cisco
enable
    clock set 20:45:00 14 March 2023
    configure terminal
        hostname sw-l2-2960-2
        service  password-encryption
        service timestamps log datetime msec 
        no ip domain-lookup
        banner motd #AVISO: acesso autorizado somente para funcionarios#
        enable secret 123@senac
        username senac secret 123@senac
        username tatuape password 123@senac
        username admin privilege 15 secret 123@senac
        line console 0 
            login local
            password 123@senac
            logging synchronous
            exec-timeout 5 30
            exit
        line vty 0 4
            login local
            password 123@senac
            logging synchronous
            exec-timeout 5 30
            transport input all
            exit
        ip default-gateway 192.168.1.254
        interface vlan 1
            description interface de svi
            ip address 192.168.1.250 255.255.255.0
            no shutdown
            end
                 
    copy running-config startup-config        
    show running-config
    show ip interface brief
    show vlan brief
    !Testando o acesso remoto no Desktop
    telnet 192.168.1.250
    telnet 192.168.1.251
